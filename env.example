# Supabase Configuration
# Obtenha estes valores no painel do Supabase (https://supabase.com/dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Exemplo:
# NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
# NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Google OAuth Configuration (opcional - se usando Google Auth)
# Configure no painel do Supabase em Authentication > Providers > Google
# NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Environment
NODE_ENV=development

# Conta Azul OAuth
NEXT_PUBLIC_CONTA_AZUL_CLIENT_ID=your_conta_azul_client_id
CONTA_AZUL_CLIENT_SECRET=your_conta_azul_client_secret
# Escopos que funcionaram no projeto anterior
CONTA_AZUL_SCOPES=openid profile aws.cognito.signin.user.admin
# URLs default podem ser sobrescritas para ambientes de sandbox (opcional)
# CONTA_AZUL_AUTH_URL=https://auth.contaazul.com/oauth2/authorize
# CONTA_AZUL_TOKEN_URL=https://api.contaazul.com/oauth2/token
# CONTA_AZUL_API_BASE_URL=https://api.contaazul.com
# CONTA_AZUL_REVOKE_URL=https://api.contaazul.com/oauth2/revoke
